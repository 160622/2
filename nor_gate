#nor_gate
import numpy as np 
##
def sigmoid (x):
    return 1/(1 + np.exp(-x))

#Input datasets
inputs = np.array([[1,1],[0,0],[1,0],[0,1]])
y = np.array([[0],[1],[0],[0]])

w= np.array([-1,-1])
b=1 

def perceptronModel(x, w, b): 
    v = np.dot(w, x) + b 
  
    py=sigmoid(v)
    return py 

for i in range(0,len(inputs)):
    x=inputs[i]
    py=perceptronModel(x, w, b)
    print("Not({}) = actual{} and predected [{}]".format(x, y[i], py)) 
